{
  "name": "@repo/frontend-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/ui/*",
    "packages/client/*",
    "tooling/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "biome check --write .",
    "lint:ci": "biome ci .",
    "format": "biome format --write .",
    "format-check": "biome format .",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules",
    "ui:add:web": "cd packages/ui/web && bunx shadcn@canary add",
    "ui:add:mobile": "cd packages/ui/mobile && bunx gluestack-ui@latest add --use-bun",
    "tokens:build": "bun run packages/tokens/scripts/generate-css.ts",
    "generate:types": "bun run scripts/generate-types.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.55.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.24",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@react-native/babel-preset": "^0.82.1",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-react-native-web": "^0.0.29",
    "@storybook/addon-themes": "^10.1.10",
    "@storybook/nextjs": "^10.1.10",
    "@storybook/react": "^10.1.10",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.1",
    "@turbo/gen": "^2.6.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-native-web": "^0.19.13",
    "jsdom": "^27.4.0",
    "react-native-css-interop": "^0.1.22",
    "react-native-web": "^0.21.2",
    "storybook": "^10.1.10",
    "tailwindcss": "^4",
    "turbo": "^2.6.0",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.19.0",
    "bun": ">=1.2.0"
  },
  "packageManager": "bun@1.2.8"
}
